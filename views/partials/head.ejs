.btn-primary {
  background-color:   #ffc107;
  border-color: #007bff;
  border-radius: 15px;
  opacity: 70%;
}

.btn-primary:hover {
  background-color: hsl(71, 86%, 38%);
  border-color: #0056b3;
  
}

.btn-primary:focus {
  box-shadow: none;
}

.btn-primary:active {
  background-color: #0062cc;
  border-color: #0056b3;
  box-shadow: none;
}

.btn-primary:not(:disabled):not(.disabled) {
  cursor: pointer;
}

.btn-primary:disabled {
  opacity: 0.65;
  cursor: not-allowed;
}

.btn-primary:focus:not(:focus-visible) {
  box-shadow: none;
}

.btn-primary:focus-visible {
  outline: 2px solid #212529;
  outline-offset: 2px;
}

.btn-primary:hover:focus-visible {
  box-shadow: none;
}

.btn-primary:active:focus-visible {
  outline: 2px solid #212529;
  outline-offset: 2px;
}

.btn-primary:focus-visible:not(:focus-within) {
  outline: none;
}

.btn-primary:focus-within:not(:focus-visible) {
  outline: 2px solid #212529;
  outline-offset: 2px;
}

.btn-primary:focus-within:focus-visible {
  outline: 2px solid #212529;
  outline-offset: 2px;
}



.btn-primary:focus-within:focus-visible:not(:focus-visible) {
  outline: none;
}
.needcard {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width:100px;
  height: 100px;
  border-radius: 50%;
  cursor: pointer;
  transition: transform 0.3s ease;
  transform: scale(1);
  background-color: rgba(241,185,65,0.9640287769784173);
}

.needcard:hover {
  transform: scale(1.1);
  animation: shake 0.5s ease-in-out;
}

.needcard .icon {
  position: relative;
  font-size: 26px;
  color: var(--black1);
  margin-top: 0;
}

.needcard .label {
  position: cover;
  font-size: 10px;
  font-weight: 5px;
  color: var(--black1);
  margin-top: 0px;
}


@keyframes shake {
  0% {
    transform: translate(1px, 1px) rotate(0deg);
  }
  10% {
    transform: translate(-1px, -2px) rotate(-1deg);
  }
  20% {
    transform: translate(-3px, 0px) rotate(1deg);
  }
  30% {
    transform: translate(3px, 2px) rotate(0deg);
  }
  40% {
    transform: translate(1px, -1px) rotate(1deg);
  }
  50% {
    transform: translate(-1px, 2px) rotate(-1deg);
  }
  60% {
    transform: translate(-3px, 1px) rotate(0deg);
  }
  70% {
    transform: translate(3px, 1px) rotate(-1deg);
  }
  80% {
    transform: translate(-1px, -1px) rotate(0deg);
  }
  90% {
    transform: translate(1px, 2px) rotate(1deg);
  }
  100% {
    transform: translate(1px, -2px) rotate(0deg);
  }
}

